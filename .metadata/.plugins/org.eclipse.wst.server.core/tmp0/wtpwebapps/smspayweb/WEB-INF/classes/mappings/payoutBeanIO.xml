<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
   
   <stream name="PayoutStream" format="csv">

      <typeHandler  name="handlerLocalDateTime" class="com.mnocompany.smspay.backend.converters.beanio.LocalDateTimeTypeHandler" />
	 
	 <record name="header" minOccurs="1" maxOccurs="1" class="com.mnocompany.smspay.backend.model.Payout">

	   <field name="userEmail" literal="User account" />
	   <field name="payoutAddress" literal="Payout Address" />
	   <field name="payoutType" literal="Payout Type"  />
	   <field name="status" literal="Status"  />
	   <field name="requestedOn" literal="Requested On [UTC]" typeHandler="handlerLocalDateTime"/>
	   <field name="requestedAmount" literal="Amount[USD]" />
	   <field name="paidOn" literal="Paid On [UTC]" typeHandler="handlerLocalDateTime" />

	  </record>
	  
	  
     <record name="details" class="com.mnocompany.smspay.backend.model.Payout" minOccurs="0">
     
		<field name="userEmail" />
	    <field name="payoutAddress" />
	    <field name="payoutType" />
	    <field name="status"   />
	    <field name="requestedOn" typeHandler="handlerLocalDateTime"/>
	    <field name="requestedAmount" />
	    <field name="paidOn"  typeHandler="handlerLocalDateTime" />
	   
	</record>
  </stream>



</beanio>
