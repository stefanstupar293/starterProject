<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
   
  <stream name="handsetSlot" format="csv">

	  <typeHandler name="dateTimeTypeHandler" class="com.mnocompany.smspay.backend.converters.beanio.LocalDateTimeTypeHandler" />

	  <record name="header" minOccurs="1" maxOccurs="1" class="com.mnocompany.smspay.backend.model.HandsetSlot">
		  <field name="id" literal="ID"/>
		  <field name="handsetId" literal="Handset ID"/>
		  <field name="imei" literal="IMEI" />
		  <field name="imsi" literal="IMSI" />
		  <field name="lastReceivedSms" literal="Last Received Sms"  typeHandler="dateTimeTypeHandler" />
		  <field name="msisdn" literal="Phone number" />
		  <segment name="mccMnc" class="com.mnocompany.smspay.backend.model.mccmnc.MccMnc">
			  <field name="mccMnc" literal="MCCMNC"/>
		  </segment>


	  </record>

	<record name="record" class="com.mnocompany.smspay.backend.model.HandsetSlot" minOccurs="0">
		<field name="id" />
		<field name="handsetId" />
		<field name="imei"  />
		<field name="imsi"   />
		<field name="lastReceivedSms"   typeHandler="dateTimeTypeHandler" />
		<field name="msisdn"  />
		<segment name="mccMnc" class="com.mnocompany.smspay.backend.model.mccmnc.MccMnc">
			<field name="mccMnc" />
		</segment>

	</record>
		

	
  </stream>




</beanio>