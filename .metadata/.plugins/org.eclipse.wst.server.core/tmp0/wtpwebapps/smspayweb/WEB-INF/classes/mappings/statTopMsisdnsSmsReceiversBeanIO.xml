<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
   
  <stream name="StatTopMsisdnsSmsReceiver" format="csv">
  
   <typeHandler  name="handlerLocalDateTime" class="com.mnocompany.smspay.backend.converters.beanio.LocalDateTimeTypeHandler" />
	 
	 <record name="header" minOccurs="1" maxOccurs="1" class="com.mnocompany.smspay.backend.model.statistics.StatTopMsisdnsSmsReceivers">

       <field name="msisdn" literal="Msisdn" />
	   <field name="countAll" literal="Count All" />
	   <field name="lastSmsTest" literal="Last Sms" typeHandler="handlerLocalDateTime" />
	   <field name="countDelivered" literal="Count Received" />
	   <field name="lastReceivedSms" literal="Last Received Sms" typeHandler="handlerLocalDateTime" />

	  </record>
	  
	  
     <record name="details" class="com.mnocompany.smspay.backend.model.statistics.StatTopMsisdnsSmsReceivers" minOccurs="0">
     
		<field name="msisdn" type="string" />
	    <field name="countAll" />
	    <field name="lastSmsTest" typeHandler="handlerLocalDateTime" />
	    <field name="countDelivered" />
	    <field name="lastReceivedSms" typeHandler="handlerLocalDateTime" />
	   
	</record>
  </stream>


</beanio>