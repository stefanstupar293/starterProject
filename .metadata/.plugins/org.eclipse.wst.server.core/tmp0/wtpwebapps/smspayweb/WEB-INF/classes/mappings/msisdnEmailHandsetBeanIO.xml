<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
   
  <stream name="MsisdnEmailHandsetStream" format="csv">

	
	 <record name="header" minOccurs="1" maxOccurs="1" class="com.mnocompany.smspay.backend.model.statistics.MsisdnEmailHandset">

	   <field name="msisdn" literal="Msisdn" />
	   <field name="email" literal="Email" />
	   <field name="imei" literal="Imei" />

	  </record>
	  
	  
     <record name="details" class="com.mnocompany.smspay.backend.model.statistics.MsisdnEmailHandset" minOccurs="0">
     
		<field name="msisdn" type="string" />
	    <field name="email" />
	    <field name="imei" />
	</record>
  </stream>


</beanio>