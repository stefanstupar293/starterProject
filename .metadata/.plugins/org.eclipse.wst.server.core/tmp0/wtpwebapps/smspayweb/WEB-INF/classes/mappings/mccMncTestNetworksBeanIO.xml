<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
   
  <stream name="mccMncTestNetworks" format="csv">

	  <record name="header" minOccurs="1" maxOccurs="1"   class="com.mnocompany.smspay.backend.model.MccMncTestNetwork" >

		  <field name="mccMnc" literal="MCC-MNC" />
		  <field name="country" literal="Country" />
		  <field name="network" literal="Network" />

		  <segment name="portedMccMnc" class="com.mnocompany.smspay.backend.model.mccmnc.MccMnc">
			  <field name="mccMnc" literal="Ported from" />
		  </segment>



	  </record>


	  <record name="details" class="com.mnocompany.smspay.backend.model.MccMncTestNetwork" >

		  <field name="mccMnc" />
		  <field name="country" />
		  <field name="network"  />

		  <segment name="portedMccMnc" class="com.mnocompany.smspay.backend.model.mccmnc.MccMnc">
		   	<field name="mccMnc"  />
		  </segment>

	  </record>

  </stream>


</beanio>