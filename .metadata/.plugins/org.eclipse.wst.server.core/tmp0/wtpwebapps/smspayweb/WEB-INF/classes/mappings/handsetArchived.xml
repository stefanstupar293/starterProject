<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
   
  <stream name="HandsetArchivedStream" format="csv">

  <typeHandler  name="handlerLocalDateTime" class="com.mnocompany.smspay.backend.converters.beanio.LocalDateTimeTypeHandler" />
 
	 <record name="header" minOccurs="1" maxOccurs="1" class="com.mnocompany.smspay.backend.model.Handset">

	   
	    <field name="userEmail" literal="User Email" />
	    <field name="lastAvailableWifi" literal="Last Available Wifi" typeHandler="handlerLocalDateTime"/>
	    <field name="lastAvailableData" literal="Last Available Data" typeHandler="handlerLocalDateTime"/>
			 
	  </record>
	  
	  
     <record name="details" class="com.mnocompany.smspay.backend.model.Handset" minOccurs="0">
     
	   <field name="userEmail"  />
	   <field name="lastAvailableWifi" typeHandler="handlerLocalDateTime" />
	   <field name="lastAvailableData" typeHandler="handlerLocalDateTime"/>
	 
	</record>
  </stream>


</beanio>