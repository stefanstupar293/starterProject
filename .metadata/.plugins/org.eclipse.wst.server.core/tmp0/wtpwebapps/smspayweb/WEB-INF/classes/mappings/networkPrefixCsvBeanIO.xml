<beanio xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.beanio.org/2012/03"
        xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

    <stream name="networkPrefixStream" format="csv">
        <parser>
            <property name="delimiter" value=";"/>
            <property name="alwaysQuote" value="true"/>
            <property name="unquotedQuotesAllowed" value="false"/>
        </parser>
        <record name="networkPrefix_header" order="1" class="com.mnocompany.smspay.backend.model.mccmnc.NetworkPrefix" minOccurs="1" maxOccurs="1">
            <field name="id" ignore="true" literal="Id" />
            <field name="prefix" ignore="true" literal="Prefix" />
            <field name="mcc"  ignore="true" literal="MCC" />
            <field name="mnc"  ignore="true"   literal="MNC" />
            <field name="countryIsoAlpha2" ignore="true"  literal="CountryIsoAlpha2"/>
            <field name="networkName" ignore="true" literal="NetworkName"/>
            <field name="fullName" ignore="true" literal="FullName"/>
            <field name="location" ignore="true" literal="Location"/>
            <field name="countryCode" ignore="true" literal="CountryCode"/>
            <field name="ndc" ignore="true" literal="NDC"/>
        </record>

        <record name="networkPrefix" order="2" class="com.mnocompany.smspay.backend.model.mccmnc.NetworkPrefix" minOccurs="0">
            <field name="id" rid="true" regex="^[0-9]*$" />
            <field name="prefix"  />
            <field name="mcc"  rid="true" regex="^[0-9]{3}$" />
            <field name="mnc" rid="true" regex="^[0-9]*$"/>
            <field name="countryIsoAlpha2"  />
            <field name="networkName"/>
            <field name="fullName"/>
            <field name="location"/>
            <field name="countryCode" />
            <field name="ndc"/>
        </record>

    </stream>

</beanio>