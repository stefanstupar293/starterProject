<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
   
  <stream name="StatTopUsersPayoutsStream" format="csv">

	 <typeHandler  name="handlerLocalDateTime" class="com.mnocompany.smspay.backend.converters.beanio.LocalDateTimeTypeHandler" />
	 
	 <record name="header" minOccurs="1" maxOccurs="1" class="com.mnocompany.smspay.backend.model.statistics.StatTopUsersPayouts">

       <field name="email" literal="Email" />
	   <field name="amount" literal="Amount" />
	   <field name="lastPayout" literal="Last Payout" typeHandler="handlerLocalDateTime" />

	  </record>
	  
	  
     <record name="details" class="com.mnocompany.smspay.backend.model.statistics.StatTopUsersPayouts" minOccurs="0">
     
         <field name="email" />
		<field name="amount" />
	    <field name="lastPayout"  typeHandler="handlerLocalDateTime" />
	   
	</record>
  </stream>


</beanio>