<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
   
   <stream name="StatTopUsersHandsetsStream" format="csv">

	
	 <record name="header" minOccurs="1" maxOccurs="1" class="com.mnocompany.smspay.backend.model.statistics.StatTopUsersHandsets"  >

	   <field name="email" literal="Email" />
	   <field name="countAll" literal="All" />
	   <field name="countActive" literal="Active" />
	   <field name="countDeleted" literal="Deleted" />
	   <field name="countBlocked" literal="Blocked" />
	 

	  </record>
	  
	  
     <record name="details" class="com.mnocompany.smspay.backend.model.statistics.StatTopUsersHandsets" minOccurs="0">
     
		<field name="email" type="string" />
	    <field name="countAll" />
		<field name="countActive" />
	    <field name="countDeleted" />
	    <field name="countBlocked" />
	   
	</record>
  </stream>



</beanio>
